<UserControl x:Class="IDservice.View.LayoutGroupsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid Background="White" Width="500px" HorizontalAlignment="Left">
        <ItemsControl ItemsSource="{Binding LayoutGroups}" Margin="20,40">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Name}" FontSize="26" FontFamily="Segoe UI Light" Margin="0,0,0,10"/>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </Grid>
</UserControl>
